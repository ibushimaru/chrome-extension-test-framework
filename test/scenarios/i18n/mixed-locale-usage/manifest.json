{
    "manifest_version": 3,
    "name": "Mixed Locale Usage Test",
    "version": "1.0.0",
    "description": "__MSG_extensionDescription__",
    "default_locale": "en",
    "action": {
        "default_popup": "popup.html",
        "default_title": "Click here"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }]
}