{
    "manifest_version": 3,
    "name": "Cross Browser Extension",
    "version": "1.0.0",
    "description": "Extension attempting cross-browser compatibility",
    "background": {
        "service_worker": "background.js"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "addon@example.com",
            "strict_min_version": "91.0"
        }
    },
    "permissions": [
        "tabs",
        "storage",
        "notifications"
    ]
}